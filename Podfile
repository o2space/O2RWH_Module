source 'https://github.com/CocoaPods/Specs.git' #官方仓库地址
source 'https://github.com/wukexiu/O2RWHSpecs.git' #私有仓库地址

platform :ios, '7.0'

workspace 'RWHModuleDemo'
project 'RWHModuleDemo.xcodeproj'
project 'RWHBaseModule.xcodeproj'

target 'RWHModuleDemo' do
platform :ios, "7.0"
project 'RWHModuleDemo.xcodeproj'
pod 'AFNetworking', '~>2.6.0'
pod 'SDWebImage', '~> 3.7.5'
pod 'Masonry','~>0.6.1'
pod 'XAspect'
pod 'Aspects'
pod 'GTSDK'
pod 'UMengAnalytics-NO-IDFA'
pod 'MJExtension', '~>3.0.10'
pod 'ActionSheetPicker' #pod 'ActionSheetPicker-3.0', '~> 2.2.0'

# 友盟分享UI
pod 'UMengUShare/UI'
# 集成新浪微博
pod 'UMengUShare/Social/Sina'
# 集成微信
pod 'UMengUShare/Social/WeChat'
# 集成QQ
pod 'UMengUShare/Social/QQ'
# 集成腾讯微博
pod 'UMengUShare/Social/TencentWeibo'

pod 'RWHBaseModule','~> 0.0.1'
end

target 'RWHBaseModule' do
project 'RWHBaseModule.xcodeproj'
pod 'XAspect'
pod 'Aspects'
pod 'UMengAnalytics-NO-IDFA', '~> 4.1.1'
pod 'GTSDK', '~> 1.5.0'
pod 'MJExtension', '~>3.0.10'
pod 'ActionSheetPicker' #pod 'ActionSheetPicker-3.0', '~> 2.2.0'
pod 'Masonry','~>0.6.1'

# 友盟分享UI
pod 'UMengUShare/UI'
# 集成新浪微博
pod 'UMengUShare/Social/Sina'
# 集成微信
pod 'UMengUShare/Social/WeChat'
# 集成QQ
pod 'UMengUShare/Social/QQ'
# 集成腾讯微博
pod 'UMengUShare/Social/TencentWeibo'
end

